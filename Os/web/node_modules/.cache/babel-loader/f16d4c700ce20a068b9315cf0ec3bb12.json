{"ast":null,"code":"import \"core-js/modules/es.number.to-fixed.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: \"Pay\",\n  data: function data() {\n    return {\n      userId: 0,\n      money1: 0,\n      orderNo: ''\n    };\n  },\n  created: function created() {\n    this.money = parseFloat(this.$route.query.money).toFixed(2);\n    this.orderNo = this.$route.query.orderNo;\n  },\n  methods: {\n    pay: function pay() {\n      // 发送支付请求\n      $.ajax({\n        url: \"/alipay/pay\",\n        // 后端处理支付请求的接口地址\n        type: \"GET\",\n        data: {\n          orderId: this.orderNo,\n          // 将订单ID发送给后端\n          goodsName: '商品名字',\n          price: this.money1\n        },\n        success: function success(data) {\n          // 支付请求成功，将支付页面展示给用户\n          $(\"body\").html(data);\n        },\n        error: function error() {\n          // 支付请求失败，提示用户支付失败\n          layer.msg(\"支付失败，请稍后重试！\");\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;AA0BA;AACAA,aADA;AAEAC,MAFA,kBAEA;AACA;AACAC,eADA;AAEAC,eAFA;AAGAC;AAHA;AAKA,GARA;AASAC,SATA,qBASA;AACA;AACA;AACA,GAZA;AAcAC;AACAC,OADA,iBACA;AACA;AACAC;AACAC,0BADA;AACA;AACAC,mBAFA;AAGAT;AACAU,+BADA;AACA;AACAC,2BAFA;AAGAC;AAHA,SAHA;AAQAC;AACA;AACAN;AACA,SAXA;AAYAO;AAEA;AACAC;AACA;AAhBA;AAkBA;AArBA;AAdA","names":["name","data","userId","money1","orderNo","created","methods","pay","$","url","type","orderId","goodsName","price","success","error","layer"],"sourceRoot":"src/views/front/order","sources":["Pay.vue"],"sourcesContent":["\r\n<template>\r\n<div style=\"margin-top: 10px;width: 50%;margin: 10px auto;background-color: white;text-align: center\">\r\n  <table style=\"margin: 20px auto\">\r\n    <tr>\r\n      <th>订单号</th>\r\n      <td>{{orderNo}}</td>\r\n    </tr>\r\n\r\n    <tr>\r\n      <th>金额</th>\r\n      <td>{{money}} 元</td>\r\n    </tr>\r\n\r\n  </table>\r\n  <hr style=\"width: 280px\" />\r\n  <div>\r\n    <span>支付方式：</span>\r\n  </div>\r\n  <br>\r\n<!--  <img src=\"../../../resource/img/微信支付.png\" style=\"width: 45px;height: 45px;margin-right: 60px;cursor:pointer;\" @click=\"pay\">-->\r\n  <img src=\"../../../resource/img/支付宝.png\" style=\"width: 50px;height: 50px;cursor:pointer;\" @click=\"pay\">\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Pay\",\r\n  data(){\r\n    return{\r\n      userId: 0,\r\n      money1: 0,\r\n      orderNo: '',\r\n    }\r\n  },\r\n  created() {\r\n    this.money = parseFloat(this.$route.query.money).toFixed(2);\r\n    this.orderNo = this.$route.query.orderNo;\r\n  },\r\n\r\n  methods:{\r\n      pay() {\r\n        // 发送支付请求\r\n        $.ajax({\r\n          url: \"/alipay/pay\", // 后端处理支付请求的接口地址\r\n          type: \"GET\",\r\n          data: {\r\n            orderId: this.orderNo, // 将订单ID发送给后端\r\n            goodsName: '商品名字',\r\n            price: this.money1,\r\n          },\r\n          success: function(data) {\r\n            // 支付请求成功，将支付页面展示给用户\r\n            $(\"body\").html(data);\r\n          },\r\n          error: function() {\r\n\r\n            // 支付请求失败，提示用户支付失败\r\n            layer.msg(\"支付失败，请稍后重试！\");\r\n          }\r\n        });\r\n      }\r\n\r\n\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\ntr{\r\n  line-height:40px;\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}